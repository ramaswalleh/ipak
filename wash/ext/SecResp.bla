DATAMODEL SecResp
LIBRARIES
    libSecRespID '..\lib\libSecRespID.lib'

PRIMARY
    SRFIRSTNAME

SECONDARY
    TRI = SRFIRSTNAME (TRIGRAM)

FIELDS
    SRLASTNAME "@BSEC RESP LAST NAME:@B" : STRING [15], EMPTY
    SRFIRSTNAME "@BSEC RESP FIRST NAME:@B" : STRING [15], EMPTY
    SRSECONDNAME "@BSEC RESP SECOND NAME:@B" : STRING [15], EMPTY
    SRID "@BSEC RESP ID:@B" : TSecRespID
ENDMODEL
